<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/registerFragment"
        android:name="my.edu.tarc.oku.RegisterFragment"
        android:label="fragment_Register">
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="my.edu.tarc.oku.LoginFragment"
        android:label="LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutUs"
        android:name="my.edu.tarc.oku.AboutUs"
        android:label="fragment_about_us"
        tools:layout="@layout/fragment_about_us" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="my.edu.tarc.oku.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_feedbackFragment"
            app:destination="@id/feedbackFragment" />
        <action
            android:id="@+id/action_homeFragment_to_memberEventInfo2"
            app:destination="@id/memberEventInfo2" />
    </fragment>
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="my.edu.tarc.oku.feedbackFragment"
        android:label="feedbackFragment" >
        <argument
            android:name="markerId"
            app:argType="string" />
        <argument
            android:name="username"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/memberEventInfo2"
        android:name="my.edu.tarc.oku.MemberEventInfo"
        android:label="MemberEventInfo">
        <argument
            android:name="eventId"
            app:argType="string" />
    </fragment>
</navigation>